<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_dashboard">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.patecan.myecommercial.View.Fragments.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_products"
        android:name="com.patecan.myecommercial.View.Fragments.ProductsFragment"
        android:label="Products"
        tools:layout="@layout/fragment_products" />

    <fragment
        android:id="@+id/navigation_orders"
        android:name="com.patecan.myecommercial.View.Fragments.OrdersFragment"
        android:label="Orders"
        tools:layout="@layout/fragment_oders" />

    <fragment
        android:id="@+id/navigation_sold_product"
        android:name="com.patecan.myecommercial.View.Fragments.SoldProductsFragment"
        android:label="Sold Product"
        tools:layout="@layout/fragment_sold_products" />
</navigation>